{
	"name": "demo",
	"description": "extension",
	"version": "1.0.1",
	"manifest_version": 3,
	"background": {
		"service_worker": "background.js"
	},
	"permissions": ["storage", "activeTab", "scripting", "contextMenus", "nativeMessaging"],
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "img/icon16.png",
			"48": "img/icon48.png",
			"128": "img/icon128.png"
		}
	},
	"icons": {
		"16": "img/icon16.png",
		"48": "img/icon48.png",
		"128": "img/icon128.png"
	},
	"options_page": "options.html"
}
